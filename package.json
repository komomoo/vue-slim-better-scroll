{
  "name": "vue-slim-better-scroll",
  "version": "1.4.4",
  "description": "💇 开箱即用的/渐进式的vue移动端滚动组件/插件，基于better-scroll",
  "author": "momoko <ko.momo@qq.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/wannaxiao/vue-slim-better-scroll"
  },
  "keywords": [
    "vue",
    "better-scroll",
    "vue-better-scroll",
    "vue-scroll",
    "vue-scroller",
    "simple-scroll"
  ],
  "main": "dist/vue-slim-better-scroll.min.js",
  "module": "dist/vue-slim-better-scroll.esm.min.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit",
    "build": "node build/build.js",
    "dev:demo": "parcel demo/complex/index.html -p 8080 --out-dir demo/complex/dist",
    "build:demo": "rm -rf demo/complex/dist/ && parcel build demo/complex/index.html --out-dir demo/complex/dist --public-url ./",
    "build:rollup": "rollup -c rollup/index.js",
    "build:page": "CONFIG_ENV=page node build/build.js",
    "dev:docs": "vuepress dev docs",
    "build:docs": "vuepress build docs",
    "release": "node build/release"
  },
  "eslintConfig": {
    "extends": [
      "@momoko/vue"
    ]
  },
  "eslintIgnore": [
    "dist"
  ],
  "babel": {
    "presets": [
      "@momoko/vue"
    ]
  },
  "browserslist": [
    "iOS >= 9",
    "Android >= 4.4",
    "IE >= 9"
  ],
  "dependencies": {
    "better-scroll": "^1.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@momoko/babel-preset-vue": "^0.2.0",
    "@momoko/eslint-config-vue": "^0.2.2",
    "dio-bundler": "^0.1.1",
    "parcel-bundler": "^1.11.0",
    "vue": "^2.6.7",
    "vue-hot-reload-api": "^2.3.2",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.7"
  }
}
